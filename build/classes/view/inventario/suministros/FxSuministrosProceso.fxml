                              <HBox style="-fx-background-color: linear-gradient(#E8EAEA 0%, #D1D6D6 100%); -fx-padding: 0.8333333333333334em;">
                                 <children>
                                    <Label styleClass="labelRobotoBold15" text="Información Adicional" textFill="#020203" />
                                 </children>
                              </HBox>
                              <VBox style="-fx-padding: 0.8333333333333334em;">
                                 <children>
                                    <VBox style="-fx-spacing: 0.8333333333333334em;">
                                       <children>
                                          <GridPane style="-fx-hgap: 0.8333333333333334em; -fx-vgap: 0.8333333333333334em;">
                                             <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                             </columnConstraints>
                                             <rowConstraints>
                                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                             </rowConstraints>
                                             <children>
                                                <Label styleClass="labelRoboto13" text="Nombre alterna" textFill="#020203">
                                                   <font>
                                                      <Font name="Roboto" size="14.0" />
                                                   </font>
                                                </Label>
                                                <TextField fx:id="txtNombreGenerico" onAction="#onActionToRegister" prefHeight="30.0" prefWidth="200.0" styleClass="text-field-normal" GridPane.rowIndex="1" />
                                                <Label styleClass="labelRoboto13" text="Marca(Precio espacio o doble click)" textFill="#020203" GridPane.rowIndex="2">
                                                   <font>
                                                      <Font name="Roboto" size="14.0" />
                                                   </font>
                                                </Label>
                                                <TextField fx:id="txtMarca" onAction="#onActionToRegister" onKeyReleased="#onKeyReleasedMarca" onKeyTyped="#onKeyTypedDetalle" onMouseClicked="#onMouseClickedMarca" prefHeight="30.0" prefWidth="200.0" styleClass="text-field-normal" GridPane.rowIndex="3" />
                                                <Label styleClass="labelRoboto13" text="Estado" textFill="#020203" GridPane.columnIndex="1">
                                                   <font>
                                                      <Font name="Roboto" size="14.0" />
                                                   </font>
                                                </Label>
                                                <ComboBox fx:id="cbEstado" maxWidth="1.7976931348623157E308" prefHeight="30.0" prefWidth="160.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                                <Label styleClass="labelRoboto13" text="Presentación(Precio espacio o doble click)" textFill="#020203" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                                   <font>
                                                      <Font name="Roboto" size="14.0" />
                                                   </font>
                                                </Label>
                                                <TextField fx:id="txtPresentacion" onAction="#onActionToRegister" onKeyReleased="#onKeyReleasedPresentacion" onKeyTyped="#onKeyTypedDetalle" onMouseClicked="#onMouseClickedPresentacion" prefHeight="30.0" prefWidth="200.0" styleClass="text-field-normal" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                             </children>
                                          </GridPane>
                                          <HBox alignment="CENTER_LEFT" style="-fx-spacing: 0.8333333333333334em;">
                                             <children>
                                                <Label styleClass="labelRoboto13" text="Clave única del producto" textFill="#020203">
                                                   <font>
                                                      <Font name="Roboto" size="14.0" />
                                                   </font>
                                                </Label>
                                                <TextField fx:id="txtClaveSat" onAction="#onActionToRegister" prefHeight="30.0" styleClass="text-field-normal" HBox.hgrow="ALWAYS" />
                                             </children>
                                          </HBox>
                                          <HBox alignment="CENTER_LEFT" style="-fx-spacing: 0.8333333333333334em;">
                                             <children>
                                                <VBox style="-fx-spacing: 0.8333333333333334em;" HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <Label styleClass="labelRoboto13" text="Descripción" textFill="#020203">
                                                         <font>
                                                            <Font name="Roboto" size="14.0" />
                                                         </font>
                                                      </Label>
                                                      <TextArea fx:id="txtDescripcion" onKeyPressed="#OnKeyPressedDescripcion" prefHeight="140.0" />
                                                   </children>
                                                </VBox>
                                             </children>
                                          </HBox>
                                          <HBox alignment="CENTER_LEFT" style="-fx-spacing: 0.8333333333333334em;">
                                             <children>
                                                <CheckBox fx:id="cbLote" mnemonicParsing="false" styleClass="check-box-contenido" text="Lote (Indica si manejara un control de lotes y caducidades para este artículo)" />
                                             </children>
                                          </HBox>
                                          <HBox alignment="CENTER_LEFT" style="-fx-spacing: 0.8333333333333334em;">
                                             <children>
                                                <Label styleClass="labelRoboto13" text="Usar en" textFill="#020203">
                                                   <font>
                                                      <Font name="Roboto" size="14.0" />
                                                   </font>
                                                </Label>
                                             </children>
                                          </HBox>
                                          <HBox alignment="CENTER_LEFT" style="-fx-spacing: 0.8333333333333334em;">
                                             <children>
                                                <RadioButton fx:id="rbTodoModulos" mnemonicParsing="false" selected="true" text="Todos los módulos" />
                                                <RadioButton fx:id="rbModuloVentas" mnemonicParsing="false" text="Solo ventas" />
                                                <RadioButton fx:id="rbModuloProduccion" mnemonicParsing="false" text="Solo producción" />
                                             </children>
                                          </HBox>
                                       </children>
                                    </VBox>
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <HBox alignment="CENTER" style="-fx-background-color: white; -fx-padding: 0.8333333333333334em; -fx-spacing: 0.8333333333333334em;">
                           <children>
                              <Button fx:id="btnRegister" mnemonicParsing="false" onAction="#onActionToRegister" onKeyPressed="#onKeyPressedToRegister" styleClass="buttonLightDefault" text="Registrar">
                                 <graphic>
                                    <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../../image/save.png" />
                                       </image>
                                    </ImageView>
                                 </graphic>
                              </Button>
                              <Button mnemonicParsing="false" onAction="#onActionToCancel" onKeyPressed="#onKeyPressedToCancel" styleClass="buttonLight" text="Cancelar">
                                 <graphic>
                                    <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../../image/error.png" />
                                       </image>
                                    </ImageView>
                                 </graphic>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </content>
</ScrollPane>
